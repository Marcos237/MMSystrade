@model Systrade.Aplicacao.ViewModel.AgenciaUsuarioViewModel


<div class="modal-header">
    <h4 class="modal-title">Adicionar usuário</h4>
    <button type="button" class="close" data-dismiss="modal" title="Fechar">
        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
    </button>
</div>

<div class="form-group-sm">
    @Html.Partial("_ErrorSummary")
</div>

<div class="modal-body">
    @using (Html.BeginForm("AdicionarAgenciaUsuario", "AgenciaUsuario"))
    {

        @Html.AntiForgeryToken()


        @Html.Hidden("AgenciaId", (Guid?)ViewBag.AgenciaId)
        @Html.Partial("_AgenciaUsuario")

        <div class="modal-footer">
            <input type="submit" value="Adicionar" class="btn btn-success" title="Adicionar" id="adicionarusuario" />
            <input type="button" class="btn btn-danger" data-dismiss="modal" value="Fechar" title="Fechar" />
        </div>
    }
</div>
<script src="~/Scripts/Agencia/DropDownClaims.js"></script>