@model IEnumerable<Systrade.Eventos.Aplicacao.ViewModel.UsuarioEventsViewModel>


<div class="modal-header">
    <h4 class="modal-title">Detalhes do usuário</h4>
    <button type="button" class="close" data-dismiss="modal" title="Fechar">
        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
    </button>
</div>

<div class="form-group-sm">
    @Html.Partial("_ErrorSummary")
</div>

<div class="modal-body">
    <div class="form-horizontal">


        @Html.Hidden("UsuarioId", (Guid?)ViewBag.UsuarioId)

        @Html.Partial("_DadosUsuarioEvents")

        <div class="modal-footer">
            <input type="button" class="btn btn-danger" data-dismiss="modal" value="Fechar" title="Fechar" />
        </div>

    </div>
</div>