@model Systrade.Clientes.Applications.ViewModel.CelulaViewModel



<div class="modal-header">
    <h4 class="modal-title">Deseja excluir essa célula?Todos os processos vinculados à ela serão excluidos! </h4>
    <button type="button" class="close" data-dismiss="modal" title="Fechar">
        <span aria-hidden="true">×</span><span class="sr-only">Close</span>
    </button>
</div>

<div class="form-group-sm">
    @Html.Partial("_ErrorSummary")
</div>

@using (Html.BeginForm("ExcluirCelula", "Celula", FormMethod.Post))
{

    <div class="modal-body">
        @Html.AntiForgeryToken()

        <div class="form-horizontal">

            @Html.Partial("_DadosCelula")

            @Html.HiddenFor(model => model.CelulaId)

            <div class="modal-footer">
                <input type="submit" value="Excluir" class="btn btn-danger" title="Excluir" />
                <input type="button" class="btn btn-primary" data-dismiss="modal" value="Fechar" title="Fechar" />
            </div>
        </div>
    </div>
}
